{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListBucketsInConsole",
      "Effect": "Allow",
      "Action": [
        "s3:ListAllMyBuckets",
        "s3:GetBucketLocation"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": [
        "arn:aws:s3:::cloudlaunch-private-bucket-426300336425",
        "arn:aws:s3:::cloudlaunch-site-bucket-426300336425",
        "arn:aws:s3:::cloudlaunch-visible-only-bucket-426300336425"
      ]
    },
    {
      "Effect": "Allow",
      "Action": ["s3:GetObject", "s3:PutObject"],
      "Resource": "arn:aws:s3:::cloudlaunch-private-bucket-426300336425/*"
    },
    {
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": "arn:aws:s3:::cloudlaunch-site-bucket-426300336425/*"
    }
  ]
}
